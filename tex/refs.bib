  keywords = {Algebraic graph theory,complex networks,networks of networks,pinning control.}
}

@article{Ma2013,
  title = {Parameter Estimation of Photovoltaic Models via Cuckoo Search},
  author = {Ma, Jieming and Ting, T. O. and Man, Ka Lok and Zhang, Nan and Guan, Sheng Uei and Wong, Prudence W.H.},
  year = {2013},
  journal = {Journal of Applied Mathematics},
  volume = {2013},
